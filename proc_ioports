0000-0cf7 : PCI Bus 0000:00
  0000-001f : dma1
  0020-0021 : pic1
  0040-0043 : timer0
  0050-0053 : timer1
  0060-0060 : keyboard
  0061-0061 : PNP0800:00
  0062-0062 : PNP0C09:00
    0062-0062 : EC data
  0064-0064 : keyboard
  0066-0066 : PNP0C09:00
    0066-0066 : EC cmd
  0070-0071 : rtc0
  0080-008f : dma page reg
  00a0-00a1 : pic2
  00c0-00df : dma2
  00f0-00ff : PNP0C04:00
    00f0-00ff : fpu
  03c0-03df : vesafb
  0800-0803 : ACPI PM1a_EVT_BLK
  0804-0805 : ACPI PM1a_CNT_BLK
  0810-0817 : ACPI GPE0_BLK
  0818-081b : ACPI PM_TMR
  0b00-0b08 : piix4_smbus
  0b20-0b28 : piix4_smbus
0cf8-0cff : PCI conf1
0d00-ffff : PCI Bus 0000:00
  1000-1fff : PCI Bus 0000:01
    1000-10ff : 0000:01:00.0
  2000-2fff : PCI Bus 0000:02
    2000-207f : 0000:02:00.0
  3000-30ff : 0000:00:01.0
  3400-340f : 0000:00:11.0
    3400-340f : ahci
  3410-3417 : 0000:00:11.0
    3410-3417 : ahci
  3418-341f : 0000:00:11.0
    3418-341f : ahci
  3420-3423 : 0000:00:11.0
    3420-3423 : ahci
  3424-3427 : 0000:00:11.0
